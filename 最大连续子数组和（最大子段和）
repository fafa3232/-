#include<stdio.h>
#include<stdlib.h>
int main()
{
	int x,i,j;
	char s[50],a[50];
	while(1)
	{
	int cnt=0,sum=0,max=0,h=0;
	scanf("%d",&x);
	for(i=0;i<x;i++)
		scanf("%d",&s[i]);
	for(i=0;i<x;i++)
	{
		if(s[i]<0)
			cnt++;
		else
			break;
	}
	if(cnt==x&&cnt!=0)
	{
		printf("0");
		system("pause");
		return 0;
	}
	for(i=0;i<x;i++)
	{
		sum=s[i];
		for(j=i+1;j<=x;j++)
		{
			a[h]=sum;
			sum=sum+s[j];
			h++; 
			a[h]=sum;
		}
		h++;
	}
	for(i=0;i<=h;i++)
	{
		if(a[i]>=max)
			max=a[i];
	}
	printf("%d",max);
	system("pause");
	}
}
