#include<stdio.h>
#include<stdlib.h>
int main()
{
	char s[20];
	int x,i,sum,cnt;
	while(1)
	{
		sum=0;
		cnt=0;
        scanf("%d",&x);
	    for(i=0;i<x;i++)
			scanf("%d",&s[i]);
	    for(i=0;i<x;i++)
	    {
		    if(s[i]<0)
				cnt++;
	    }
	    if(cnt==x)
	    {
		    printf("0");
		    system("pause");
		    return 0;
	    }
	    for(i=0;i<x;i++)
			sum=sum+s[i];
	   printf("%d",sum);
	   system("pause");
    }
}
